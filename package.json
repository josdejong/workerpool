{
  "name": "@danielsimonjr/workerpool",
  "license": "Apache-2.0",
  "version": "10.0.1",
  "description": "Offload tasks to a pool of workers on node.js and in the browser",
  "homepage": "https://github.com/danielsimonjr/workerpool",
  "author": "Jos de Jong <wjosdejong@gmail.com> (https://github.com/josdejong)",
  "contributors": [
    "Daniel Simon Jr (https://github.com/danielsimonjr)"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/danielsimonjr/workerpool.git"
  },
  "keywords": [
    "worker",
    "web worker",
    "cluster",
    "pool",
    "isomorphic"
  ],
  "main": "src/js/index.js",
  "browser": "dist/workerpool.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./src/js/index.js",
      "require": "./src/js/index.js",
      "browser": "./dist/workerpool.js"
    },
    "./modern": {
      "types": "./types/ts/index.d.ts",
      "import": "./dist/ts/index.js",
      "require": "./dist/ts/index.js"
    },
    "./minimal": {
      "types": "./types/ts/minimal.d.ts",
      "import": "./dist/ts/minimal.js",
      "require": "./dist/ts/minimal.js"
    },
    "./full": {
      "types": "./types/ts/full.d.ts",
      "import": "./dist/ts/full.js",
      "require": "./dist/ts/full.js"
    },
    "./wasm": {
      "types": "./types/ts/wasm/index.d.ts",
      "import": "./dist/ts/wasm/index.js",
      "require": "./dist/ts/wasm/index.js"
    },
    "./errors": {
      "types": "./types/ts/errors.d.ts",
      "import": "./dist/ts/errors.js",
      "require": "./dist/ts/errors.js"
    },
    "./debug": {
      "types": "./types/ts/debug.d.ts",
      "import": "./dist/ts/debug.js",
      "require": "./dist/ts/debug.js"
    }
  },
  "files": [
    "dist",
    "src",
    "types",
    "HISTORY.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "node scripts/build-js.mjs",
    "build:js": "node scripts/build-js.mjs",
    "build:js:watch": "node scripts/build-js.mjs --watch",
    "build:ts": "node scripts/build-js.mjs --ts",
    "build:ts:watch": "node scripts/build-js.mjs --ts --watch",
    "build:types": "node scripts/build-js.mjs --types",
    "build:wasm": "node scripts/build-wasm.mjs --release",
    "build:wasm:debug": "node scripts/build-wasm.mjs --debug",
    "build:wasm:esm": "node scripts/build-wasm.mjs --esm",
    "build:wasm:raw": "node scripts/build-wasm.mjs --raw",
    "build:wasm:all": "node scripts/build-wasm.mjs --all",
    "build:wasm:embed": "node scripts/build-wasm.mjs --all --embed",
    "build:wasm:validate": "node scripts/build-wasm.mjs --all --validate",
    "build:wasm:clean": "node scripts/build-wasm.mjs --clean --all",
    "build:wasm:watch": "node scripts/build-wasm.mjs --watch",
    "build:ts:full": "npm run build:wasm:embed && npm run build:ts",
    "typecheck": "tsc --noEmit",
    "typecheck:wasm": "tsc -p src/ts/assembly/tsconfig.json --noEmit",
    "watch": "rollup -c rollup.config.mjs -w",
    "watch:wasm": "node scripts/build-wasm.mjs --watch",
    "test": "npm run build && npm run test:js && npm run test:ts && npm run test:types",
    "test:js": "mocha test/js",
    "test:ts": "vitest run",
    "test:types": "tsc -p test/js/types",
    "test:wasm": "npm run build:wasm && tsx node_modules/mocha/bin/mocha test/js/wasm.test.js",
    "test:debug": "npm run build && mocha debug test/js",
    "coverage": "npm run build && c8 mocha test/js && c8 report --reporter=html && echo Coverage report is available at ./coverage/index.html",
    "clean": "rm -rf dist/*.wasm dist/*.wat dist/*.wasm.map",
    "clean:all": "rm -rf dist node_modules/.cache",
    "validate:wasm": "node scripts/validate-wasm.mjs",
    "prepublishOnly": "npm run test && npm run build:all"
  },
  "devDependencies": {
    "@babel/core": "7.28.5",
    "@babel/preset-env": "7.28.5",
    "@rollup/plugin-babel": "6.1.0",
    "@rollup/plugin-commonjs": "29.0.0",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "16.0.3",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "12.1.2",
    "@types/node": "24.10.1",
    "@vitest/coverage-v8": "^4.0.16",
    "assemblyscript": "^0.28.9",
    "c8": "10.1.3",
    "core-js": "3.47.0",
    "date-format": "4.0.14",
    "find-process": "2.0.0",
    "fs-extra": "11.3.2",
    "mocha": "11.7.5",
    "rollup": "4.53.3",
    "tslib": "^2.8.1",
    "tsx": "^4.21.0",
    "typescript": "5.9.3"
  }
}
